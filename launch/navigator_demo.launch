<?xml version='1.0'?>
<launch>
	<!-- Navigator Node -->
	<node pkg="egh450_navigation_interface" type="navigator" name="navigator" clear_params="true" output="screen">
		<!-- These topics need to be remapped correctly! -->
		<!-- Input to trigger diversion from imagery -->
		<remap from="~imagery_trigger" to="/imagery_trigger" />
		<!-- Feedback from contrail to alert us when waypoints have been reached -->
		<remap from="~discrete_progress" to="/discrete_progress" />
		<!-- Pose reference input to contrail to perform diversion -->
		<remap from="~pose" to="/pose" />
		<!-- Service to switch contrail between tracking modes -->
		<remap from="~set_tracking" to="/set_tracking" />
	</node>
</launch>
